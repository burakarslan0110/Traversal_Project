@model List<EntityLayer.Concrete.Contact>

@foreach (var item in Model)
{
    <div class="col-lg-6 contact-left">
        <div class="partners">
            <div class="cont-details">
                <h5>Bize Ulaşın!</h5>
                <p class="mt-3 mb-4">
                    @item.Description
                </p>
            </div>
            <div class="hours">
                <h6 class="mt-4">E-posta Adresimiz: </h6>
                <p style="color:red">@item.Mail</p>
                <h6 class="mt-4">Telefon Numaramız: </h6>
                <p><a href="tel:" style="color:red">@item.Phone</a></p>
                <h6 class="mt-4">Ofis Adresimiz: </h6>
                <p><a href="@item.MapLocation" target="_blank" style="color:red">@item.Address</a></p>
            </div>
        </div>
    </div>
}
